{
    "name": "pkg-size",
    "displayName": "pkg-size",
    "description": "",
    "version": "1.0.0",
    "keywords": [
        "package",
        "size"
    ],
    "icon": "icon/pkg-size.png",
    "publisher": "zingerbee",
    "author": "ZingerLittleBee",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/ZingerLittleBee/pkg-size.git"
    },
    "bugs": {
        "url": "https://github.com/ZingerLittleBee/pkg-size/issues"
    },
    "homepage": "https://github.com/ZingerLittleBee/pkg-size",
    "engines": {
        "vscode": "^1.68.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "pkg-size.rebuild",
                "title": "Rebuild Deps"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run package",
        "webpack": "webpack --mode development",
        "webpack-dev": "webpack --mode development --watch",
        "package": "webpack --mode production --no-devtool",
        "test-compile": "tsc -p ./",
        "cz": "git-cz",
        "pretest": "yarn run compile && yarn run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.1.1",
        "@types/node": "16.x",
        "@types/vscode": "^1.68.0",
        "@typescript-eslint/eslint-plugin": "^5.27.0",
        "@typescript-eslint/parser": "^5.27.0",
        "@vscode/test-electron": "^2.1.3",
        "commitizen": "^4.2.4",
        "eslint": "^8.16.0",
        "git-cz": "^4.9.0",
        "glob": "^8.0.3",
        "mocha": "^10.0.0",
        "ts-loader": "^9.3.1",
        "typescript": "^4.7.2",
        "webpack": "^5.73.0",
        "webpack-cli": "^4.10.0"
    },
    "config": {
        "commitizen": {
            "path": "git-cz"
        }
    },
    "dependencies": {
        "axios": "^0.27.2"
    }
}